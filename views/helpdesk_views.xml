<odoo>
  <data>
    <record id="cl_helpdesk_stage_form_ans_id" model="ir.ui.view">
      <field name="name">cl_helpdesk.stage.form.ans.id</field>
      <field name="model">helpdesk.stage</field>
      <field name="inherit_id" ref="helpdesk.helpdesk_stage_view_form"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='is_close']" position="after">
          <field name="sla_id"/>
          <field name="def_assign" domain="[('share','=',False)]"/>
        </xpath>
      </field>
    </record>
    <record id="cl_helpdesk_ticket_form_inherit_timesheet_tickets_anteriores" model="ir.ui.view">
      <field name="name">cl_helpdesk.ticket.form.inherit.timesheet.tickets.anteriores</field>
      <field name="model">helpdesk.ticket</field>
      <field name="inherit_id" ref="helpdesk_timesheet.helpdesk_ticket_view_form_inherit_helpdesk_timesheet"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='description']" position="after">
          <notebook>
            <page string="Otros tickets del lote">
              <field name="historial_tickets"/>
            </page>
          </notebook>
        </xpath>
      </field>
    </record>
  </data>
</odoo>