<odoo>
  <data>
    <record id="cl_helpdesk_stage_form_ans_id" model="ir.ui.view">
      <field name="name">cl_helpdesk.stage.form.ans.id</field>
      <field name="model">helpdesk.stage</field>
      <field name="inherit_id" ref="helpdesk.helpdesk_stage_view_form"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='is_close']" position="after">
          <field name="sla_id"/>
          <field name="def_assign" domain="[('share','=',False)]"/>
        </xpath>
      </field>
    </record>
    <record id="cl_helpdesk_ticket_form_tickets_anteriores" model="ir.ui.view">
      <field name="name">cl_helpdesk.ticket.form.tickets.anteriores</field>
      <field name="model">helpdesk.ticket</field>
      <field name="inherit_id" ref="helpdesk.helpdesk_ticket_view_form"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='x_lot_id']" position="after">
            <field name="historial_tickets">
              <tree>
                <field name="id" string="Ref"/>
                <field name="name"/>
                <field name="user_id"/>
                <field name="stage_id"/>
                <field name="active"/><!--temp-->
              </tree>
            </field>
        </xpath>
      </field>
    </record>
  </data>
</odoo>